# Checking the length of both lists to ensure they match
universities = [
    "University of Southern California", "University of North Carolina at Chapel Hill", "University of Florida",
    "Syracuse University", "University of Texas at Austin", "Northwestern University", "University of Missouri",
    "Michigan State University", "University of Wisconsin-Madison", "Indiana University Bloomington",
    "University of Georgia", "University of Maryland, College Park", "University of Minnesota Twin Cities",
    "Pennsylvania State University", "Boston University", "University of Miami", "Ohio University",
    "University of Illinois at Urbana-Champaign", "Florida State University", "University of Iowa",
    "University of Kansas", "University of Kentucky", "University of Alabama", "Temple University",
    "University of Colorado Boulder", "University of South Carolina", "San Diego State University",
    "University of Connecticut", "Louisiana State University", "Arizona State University", "University of Oklahoma",
    "University of Utah", "University of Tennessee", "University of Arkansas", "University of Oregon",
    "Washington State University", "University of Nebraska-Lincoln", "University of Nevada, Reno",
    "University of Massachusetts Amherst", "University of Delaware", "California State University, Fullerton",
    "University of Houston", "University of Cincinnati", "University of Louisville", "University of Central Florida",
    "San Francisco State University", "University of New Mexico", "University of South Florida",
    "Iowa State University", "University of North Texas", "University of Memphis", "University of Alabama at Birmingham"
]

acceptance_rates = [
    12, 25, 31, 44, 29, 9, 81, 71, 54, 77, 48, 44, 57, 54, 19, 33, 78, 63, 37, 82, 93, 95, 80, 71, 79, 68, 37, 56, 74, 88, 
    80, 62, 78, 78, 84, 80, 78, 85, 65, 81, 63, 77, 76, 79, 45, 73, 94, 77, 75, 94
]

