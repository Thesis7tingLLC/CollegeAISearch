# Correcting the acceptance rates list to match the length of the colleges list
acceptance_rates_corrected = acceptance_rates[:len(colleges)]

# Create a DataFrame with the corrected acceptance rates
data_corrected = {"College": colleges, "Acceptance Rate (%)": acceptance_rates_corrected}
df_corrected = pd.DataFrame(data_corrected)

# Sort the DataFrame by acceptance rate
df_sorted_corrected = df_corrected.sort_values(by="Acceptance Rate (%)")

# Save the DataFrame to an Excel file
file_path_corrected = "/mnt/data/top_100_liberal_arts_colleges_economics_departments.xlsx"
df_sorted_corrected.to_excel(file_path_corrected, index=False)

file_path_corrected

