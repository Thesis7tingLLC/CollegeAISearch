# Adding two more universities and their corresponding acceptance rates to make the list top 100
additional_universities = [
    "University of Hawaii at Manoa", "University of Louisville"
]

additional_acceptance_rates = [
    82.0, 74.2
]

# Update the lists
universities_100 = universities_trimmed + additional_universities
acceptance_rates_100 = acceptance_rates + additional_acceptance_rates

# Create DataFrame with the updated lists
df_top_100 = pd.DataFrame({
    "University": universities_100,
    "Acceptance Rate (%)": acceptance_rates_100
})

# Sort by Acceptance Rate in ascending order
df_sorted_top_100 = df_top_100.sort_values(by="Acceptance Rate (%)").reset_index(drop=True)

# Save to an Excel file
file_path_top_100 = "/mnt/data/Top_100_Political_Science_Departments.xlsx"
df_sorted_top_100.to_excel(file_path_top_100, index=False)

