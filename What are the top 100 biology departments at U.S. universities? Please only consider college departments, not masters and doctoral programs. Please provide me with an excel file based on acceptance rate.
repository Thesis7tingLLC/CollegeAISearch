# Truncate the acceptance_rates list to match the length of universities list (99 elements)
acceptance_rates = acceptance_rates[:99]

# Creating the DataFrame again
df = pd.DataFrame({"University": universities, "Approx. Acceptance Rate (%)": acceptance_rates})

# Sorting by acceptance rate
df_sorted = df.sort_values(by="Approx. Acceptance Rate (%)")

# Saving to an Excel file
file_path = "/mnt/data/Top_100_Biology_Departments.xlsx"
df_sorted.to_excel(file_path, index=False)

